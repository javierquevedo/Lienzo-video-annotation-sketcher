
<div id="page">
<table>
  <tr>
    <td><%= image_tag('clipIcon.png') %></td>
    <td><h1> Latest clips </h1></td>
</table>

<table>
  <% for clip in @clips %>
    <tr>
      <td><%= link_to clip.name, clip %></td>
      <td><%= clip.annotations.count %> annotations</td>

    </tr>

  <% end %>
</table>
<br />
</div>

<div class="linkBar" >
<table cellpadding="0" cellspacing="0"><tr>
  <td><%= link_to image_tag('newClipIcon.png', :border=> 0) , new_clip_path %></td>
</tr>
</table>
</div>

